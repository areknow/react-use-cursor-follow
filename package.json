{
  "name": "react-use-cursor-follow",
  "version": "1.0.0",
  "description": "A React hook that creates a smooth customizable animated cursor element that follows your mouse",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,scss,md}\" \"*.md\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,scss,md}\" \"*.md\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build"
  },
  "keywords": [
    "react",
    "component",
    "animation",
    "mouse",
    "cursor",
    "follow",
    "hook",
    "interactive"
  ],
  "author": "areknow",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/areknow/react-use-cursor-follow.git"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "classnames": "^2.5.1",
    "css-loader": "^6.8.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jest": "^29.7.0",
    "prettier": "^3.6.2",
    "react": "^18.2.0",
    "react-docgen-typescript": "^2.4.0",
    "react-dom": "^18.2.0",
    "react-styleguidist": "^13.1.4",
    "rollup": "^4.9.1",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "sass": "^1.89.2",
    "sass-loader": "^14.1.1",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8"
  }
}
